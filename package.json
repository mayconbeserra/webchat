{
  "name": "webchat",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "client:dev": "webpack -d --config client/webpack.config.js --watch",
    "client:dev:watch": "concurrently --kill-others \"npm run client:dev\" \"npm run client:dev:run\"",
    "client:dev:run": "cd client && webpack-dev-server -d --config webpack.config.js --watch --progress --colors",
    "client:build": "webpack -p --config client/webpack.config.js",
    "dev": "node ./server/index.js",
    "dev:watch": "export NODE_ENV=development && supervisor --watch server/src -- -r 'babel-register' ./server/bin/service.js",
    "lint": "eslint --ext .js --ext .jsx src/",
    "server:compile": "babel --presets es2015,stage-0 -d server/lib/ server/src/",
    "server:build": "babel server/src/ --out-dir server/lib",
    "server:test": "export NODE_ENV=test && npm run server:compile && mocha --compilers js:babel-core/register --recursive --watch-extensions .spec.js ./server/tests",
    "server:test:watch": "npm run server:test -- --watch --bail ./server/tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayconbeserra/webchat.git"
  },
  "keywords": [
    "webchat"
  ],
  "author": "Maycon Beserra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mayconbeserra/webchat/issues"
  },
  "homepage": "https://github.com/mayconbeserra/webchat#readme",
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-runtime": "^6.22.0",
    "body-parser": "^1.16.1",
    "bootstrap": "^3.3.7",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "css-loader": "^0.27.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.14.1",
    "express-validator": "^3.1.2",
    "file-loader": "^0.10.1",
    "http": "0.0.0",
    "knex": "^0.12.6",
    "node-sass": "^4.5.0",
    "pg": "^6.1.2",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "sass-loader": "^6.0.3",
    "sinon": "^1.17.7",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "style-loader": "^0.13.2",
    "supervisor": "^0.12.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.0",
    "mocha": "^3.2.0",
    "supertest": "^3.0.0"
  }
}
