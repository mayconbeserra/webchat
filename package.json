  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mayconbeserra/webchat/issues"
  },
  "homepage": "https://github.com/mayconbeserra/webchat#readme",
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "body-parser": "^1.16.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "express": "^4.14.1",
    "express-validator": "^3.1.2",
    "knex": "^0.12.6",
    "pg": "^6.1.2",
    "sinon": "^1.17.7",
    "socket.io": "^1.7.3",
    "supervisor": "^0.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "supertest": "^3.0.0"
